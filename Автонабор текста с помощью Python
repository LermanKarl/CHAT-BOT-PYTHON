import pyautogui
from time import sleep

sleep(2)
pyautogui.typewrite(
    'hello...\n'
    'my name is ....\n'
    'feel much better equipped now to handle the challenges. And if we run into an issue, we’ll come to Caktus to hammer it out," said Stephen Johnston, VP of Engineering, at Force Therapeutics... \n\n',
    interval = 0.25
)  

# визуальное отображение процесса работы
for i in tqdm (range(1, 1000)):
    sleep(0.01)
